/* pages/first/first.wxss */
page{
  background-color: #ececec
}
.tltle_main{
  width: 100%;
  height: 80rpx;
  margin-top: 20rpx;
  text-align: center;
}
.title_style{
  font-size: 50rpx;
  font-family:"", Times, serif;
  line-height: 80rpx;
  vertical-align: middle;
  color: #666666
}
.all_brand_main{
  width: 100%;
  height: 100%;
  margin-top: 20rpx;
  display: flex;
  flex-wrap: wrap;
  align-items: center;
}
.brand_item_main{
  width: 345rpx;
  height: 345rpx;
  margin-left: 20rpx;
  margin-bottom: 20rpx;
  background-color: white;
  border-right-style:solid;
  border-bottom-style:solid;
  border-left-style:solid;
  border-top-style:solid;
  border-width: 1rpx;
  border-color: #eeeeee;
}
.brand_item_image_main{
  width: 100%;
  height: 200rpx;
  margin-top: 50rpx;
  text-align: center
}
.brand_item_image_size{
  width: 200rpx;
  height: 200rpx;
  border-radius: 20rpx;
}
.brand_item_text_main{
  width: 100%;
  height: 40rpx;
  margin-top: 10rpx;
  text-align: center
}
.brand_item_text_style{
  font-size: 30rpx;
  font-family:"", Times, serif;
  line-height: 40rpx;
  vertical-align: middle;
  color: #666666
}
.skip_main{
  width: 250rpx;
  height: 80rpx;
  position: fixed;
  bottom: 80rpx;
  right: 10rpx;
  border-right-style:solid;
  border-bottom-style:solid;
  border-left-style:solid;
  border-top-style:solid;
  border-width: 1rpx;
  border-color: #aaaaaa;
  text-align: center;
}
.skip_text_style{
  font-size: 30rpx;
  font-family:"", Times, serif;
  line-height: 80rpx;
  vertical-align: middle;
  color: #666666
}
.enter_home_page_main{
  width: 100%;
  height: 120rpx;
  position: fixed;
  bottom: -1rpx;
  display: flex;
  align-items: center;
  background-color: #f6f6f6;
}
.enter_home_page_botton_main{
  width: 350rpx;
  height: 80rpx;
  margin-left: 200rpx;
  border-right-style:solid;
  border-bottom-style:solid;
  border-left-style:solid;
  border-top-style:solid;
  border-width: 1rpx;
  border-color: #22393c;
  text-align: center;
  background-color: #22393c;
  border-radius: 10rpx;
}
.enter_home_page_botton_text{
  font-size: 30rpx;
  font-family:"", Times, serif;
  line-height: 80rpx;
  vertical-align: middle;
  color: white
}